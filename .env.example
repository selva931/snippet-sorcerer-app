# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AI Provider Configuration (choose one or multiple)
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key

# Model Provider Selection
MODEL_PROVIDER=openai  # Options: openai, anthropic, mock
MODEL_NAME=gpt-4o-mini  # Optional: override default model

# Development Settings
NODE_ENV=development
ALLOW_LOG=false  # Set to true to enable code logging for debugging
PORT=3001

# Database URL for local development (optional)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/postgres